        img3 = loadImage('C:\Users\Night\Documents\GitHub\Mart220\Week3\Images\pasta.jpg')
      }
      function preload(){
        myfont1 = loadFont('C:\Users\Night\Documents\GitHub\Mart220\Week3\fonts\Abel-Regular.ttf')
      }
