      function setup(){
        createCanvas (500,500);
        setInterval(spookyTimer,1000);
      }

      function draw(){
        background(220);
        drawCounter();
        image(img1,timerX,timerY,100,100);
        image(img2,300,300);
        image(img3,250,400);

        movement();

        textFont(myFont1);
        textSize(15);
        fill(300,100,100);
        text('Damian Beardslee', 250, 400);
        textFont(myFont2);
        textSize(20);
        text('Extra Cheesy Alfredo',10,30);
      }

      function movement(){
        if(timerValue >= 20){
          timerX++;
          timerY--;
        }
      }
